    "js",
    "compile",
    "inline",
    "mobilize",
    "optimize",
    "optimization"
  ],
  "license": "MIT",
  "main": "index",
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudflare/collapsify.git"
  },
  "devDependencies": {
    "assume": "^1.2.0",
    "bl": "^0.9.4",
    "coveralls": "^2.11.4",
    "eslint": "^0.17.1",
    "istanbul": "^0.3.20",
    "jsfmt": "^0.4.1",
    "mocha": "^2.2.1",
    "mz": "^1.3.0"
  },
  "scripts": {
    "lint": "eslint bin/ lib/ tests/",
    "format": "jsfmt -w bin/ lib/ tests/",
    "start": "./bin/server.js",
    "prepublish": "./versionify.js",
    "coverage": "npm test && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "test": "istanbul cover _mocha -- -R spec tests"
  }
}
